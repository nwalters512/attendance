<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <a class="navbar-brand" href="/">Attendance</a>
  <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav">
          <% if (_user) { %>
          <li class="nav-item active">
              <a class="nav-link" href="/link">My Classes</a>
          </li>

          <% } else {%>
          <li class="nav-item active">
              <a class="nav-link" href="/register"> Register </a>
          </li>
          <li class="nav-item active">
              <a class="nav-link" href="/login"> Login </a>
          </li>
          <% } %>
      </ul>

      <% if (_user) {%>
      <div style="margin-left:6em">
          <span style="color:white"><%= _user.email %></span>
      </div>
      <a class="btn btn-primary" style="margin-left:1em" href="/logout"> Logout </a>
      <%} %>
</nav>

<%
  _err.forEach( (e) => {
%>

<p style="background-color:red"> <%= e %> </p>

<%
  });
%>

<%
  _info.forEach( (e) => {
%>

<p style="background-color:green"> <%= e %> </p>

<%
  });
%>
