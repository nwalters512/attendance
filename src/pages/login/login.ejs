<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head') %>
  </head>
  <body>
    <%- include('../partials/header') %>
    <div class="container">
				<br/>
			<form method="POST">
    	  <div class="form-group">
    	    <label for="username">Email</label>
    	    <input
    	      class="form-control"
    	      name="username"
    	      id="username"
    	    />
    	  </div>
    	  <div class="form-group">
    	    <label for="password">Password</label>
    	    <input
    	      class="form-control"
    	      name="password"
    	      id="password"
    	      placeholder=""
    	    />
    	  </div>
    	  <button type="submit" class="btn btn-primary">Login</button>
    	</form>
    </div>
  </body>
</html>
